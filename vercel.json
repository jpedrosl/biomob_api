{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "env": {
    "DB_HOST": "@db_host",
    "DB_PORT": "@db_port",
    "DB_DATABASE": "@db_database",
    "DB_USER": "@db_user",
    "DB_PASSWORD": "@db_password",
    "PORT": "@port",
    "JWT_PASS": "@jwt_pass",
    "JWT_EXPIRES": "@jwt_expires"
  }
}